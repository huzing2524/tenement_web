/**index.wxss**/
.nav{
  width: 100%;
  height: 86rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  position: fixed;
  left: 0;
  bottom: 0;
}
.nav view{
  width: 100%;
  height: 100%;
  font-size: 35rpx;
  text-align: center;
  line-height: 86rpx;
  color: #fff
}
.nav view:nth-child(1){
  background: #ff9700
}
.nav view:nth-child(2){
  background: #03bbd5
}

.container{
  position: absolute;
  left:0;
  top:0;
  bottom:86rpx;
  right: 0;
}

.container map{
  width: 100%;
  height: 100%;
}