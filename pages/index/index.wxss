.item{
  display: flex;
  padding: 20rpx 40rpx;
  border-bottom: 1rpx solid #eee;
  cursor: pointer;
}
.item .my_image{
  width: 128rpx;
  height: 168rpx;
  margin-right: 20rpx;
}
.item .meta{
  flex: 1;
}
.item .meta .title{
  display: block;
  margin-bottom: 15rpx;
}
.item .meta .title{
  font-size: 32rpx;
  font-weight: bold;
}
.item .meta .info{
  display: block;
  font-size: 22rpx;
  color: #3b3b3b;
}

